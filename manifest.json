{
    "manifest_version": 2,
    "name": "Cloud Conformity Dashboard",
    "description": "Turns the Cloud Conformity Dashboard into a dynamic page, useful for monitoring screens.",
    "version": "1.0",

    "permissions": [
        "activeTab"
    ],

    "icons": {
        "16":"resources/icon-16.png",
        "48":"resources/icon-48.png"
    },

    "content_scripts": [
        {
            "matches": [
                "https://*.cloudconformity.com/*"
            ],
            "js": ["dashboard.js"]
        }
    ]
}